{
  "name": "bytic/utility",
  "type": "library",
  "description": "Php Framework",
  "homepage": "https://github.com/bytic/utility",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\Utility\\": "src"
    },
    "classmap": [
      "legacy"
    ],
    "files": [
      "src/helpers/general.php",
      "src/helpers/input.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Utility\\Tests\\Fixtures\\": "tests/fixtures",
      "Nip\\Utility\\Tests\\": "tests/src"
    }
  },
  "require": {
    "php": "^7.1",
    "nesbot/carbon": "^2.0",
    "ramsey/uuid": "^3.7|^4.0"
  },
  "require-dev": {
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-simplexml": "*",
    "bytic/container": "^2.0.0",
    "bytic/inflector": "^1.0",
    "phpunit/phpunit": "^7.2|^8.0|^9.0",
    "mockery/mockery": "^1.0"
  },
  "replace": {
    "bytic/utility": "^1.0"
  }
}