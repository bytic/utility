{
  "name": "bytic/utility",
  "type": "library",
  "description": "Php Framework",
  "homepage": "https://github.com/bytic/utility",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\Utility\\": "src"
    },
    "classmap": [
      "legacy"
    ],
    "files": [
      "src/helpers/general.php",
      "src/helpers/input.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Utility\\Tests\\Fixtures\\": "tests/fixtures",
      "Nip\\Utility\\Tests\\": "tests/src"
    }
  },
  "require": {
    "php": "^8.0",
    "nesbot/carbon": "^2.0",
    "ramsey/uuid": "^3.7|^4.0",
    "voku/portable-ascii": "^1.4|^2.0",
    "league/iso3166": "^3.0|^4.0"
  },
  "require-dev": {
    "ext-json": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-mbstring": "*",
    "ext-simplexml": "*",
    "bytic/container": "^2.0",
    "bytic/inflector": "^1.0",
    "bytic/contracts": "^1.0",
    "bytic/phpqatools": "^1.0",
    "symfony/http-foundation": "^4.0|^5.0|^6.0"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "bytic/phpqatools": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
